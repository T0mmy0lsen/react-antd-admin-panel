{
  "name": "examples",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://localhost:8000",
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "@craco/craco": "^7.1.0",
    "@fortawesome/fontawesome-svg-core": "^6.5.1",
    "@fortawesome/free-brands-svg-icons": "^6.5.1",
    "@fortawesome/free-regular-svg-icons": "^6.5.1",
    "@fortawesome/free-solid-svg-icons": "^6.5.1",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@rollup/plugin-commonjs": "^22.0.1",
    "@types/estree": "^1.0.0",
    "@types/node": "^18.6.2",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "antd": "^5.12.1",
    "array-move": "^4.0.0",
    "axios": "^0.27.2",
    "craco-less": "^3.0.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "react": "^18.2.0",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.3.0",
    "tslib": "^2.4.0",
    "typescript": "^4.7.4"
  },
  "scripts": {
    "start": "craco start",
    "build": "cross-env NODE_ENV=production craco build",
    "push": "git add ../ && git commit -m \"push\" && git push",
    "postbuild": "npm run deploy",
    "deploy": "node deploy.js",
    "deploy:html": "npx cpy build/index.html ../backend/resources/views --rename=react.blade.php && npx shx mv ..\\backend\\resources\\views\\build\\react.blade.php ..\\backend\\resources\\views\\react.blade.php",
    "deploy:assets": "npx cpy build/**/* ../backend/public --parents",
    "test": "craco test",
    "eject": "craco eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "cpy": "^11.0.0",
    "cross-env": "^7.0.3",
    "shx": "^0.3.4"
  }
}
